{
  "name": "test website",
  "version": "1.0.0",
  "_moduleAliases": {
    "@": "./dist"
  },
  "scripts": {
    "build": "tsc",
    "start": "node -r module-alias/register dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node -r module-alias/register dist/main.js",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "seed": "ts-node -r tsconfig-paths/register src/seed.ts",
    "seed:undo": "npx sequelize-cli db:seed:undo:all --seeders-path src/database/seeders",
    "migration:create": "npx sequelize-cli migration:generate --name",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "migrate:to": "npx sequelize-cli db:migrate --to",
    "migrate:undo:to": "npx sequelize-cli db:migrate:undo --to"
  },
  "dependencies": {
    "@faker-js/faker": "^9.2.0",
    "@nestjs/common": "^10.4.8",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.4.8",
    "@nestjs/sequelize": "^10.0.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "handlebars": "^4.7.8",
    "joi": "^17.13.3",
    "module-alias": "^2.2.3",
    "mysql2": "^3.11.4",
    "nodemailer": "^6.9.16",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/hapi__joi": "^17.1.15",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "mkdirp": "^3.0.1",
    "prettier": "^3.3.3",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
